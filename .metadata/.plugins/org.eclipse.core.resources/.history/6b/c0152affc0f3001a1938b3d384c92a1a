package day.twelve;

public class EmployeeDemo {

	public static void main(String[] args) {
		FullTimeEmployee fullTimeEmployee = new FullTimeEmployee(100,"SACHIN","TENDULKAR","10-10,WEST,MUMBAI","9898766656",9999.99,2000);
		fullTimeEmployee.showDetails();
		fullTimeEmployee.sendMessage();
		
		PartTimeEmployee partTimeEmployee = new PartTimeEmployee(101,"MS","DHONI","20-20,EAST,MUMBAI","90001020",80,120);
		partTimeEmployee.showDetails();
		partTimeEmployee.sendMessage();
	}

}
